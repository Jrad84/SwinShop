<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/afterLogin/*</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>logout.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{myProductManagedBean.displayProduct()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/productInfo.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.displayDetails()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/userDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Client Only</description>
        <from-view-id>/client/*</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>logout.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.displayDetails()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/userDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Admin Only</description>
        <from-view-id>/admin/*</from-view-id>
         <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>logout.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.displayDetails()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/userDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>From Home Page</description>
        <from-view-id>/homePage.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{myProductManagedBean.displayProduct()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/productInfo.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Sign up</description>
        <from-view-id>/signupPage.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.addUser()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/signupSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.addUser()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/signupFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Update User Details</description>
        <from-view-id>/afterLogin/updateDetails.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.editDetails()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/updateDetailsSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.editDetails()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/afterLogin/updateDetailsFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Change Password</description>
        <from-view-id>/afterLogin/updatePassword.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.editPassword()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/updatePasswordSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.editPassword()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/afterLogin/updatePasswordFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Request Recover Password</description>
        <from-view-id>/changePasswordRequest.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.checkEmail()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/changePasswordValidateCode.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.checkUserEmail()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/changePasswordRequestFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Validate Recovery Code</description>
        <from-view-id>/changePasswordValidateCode.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.checkRecoveryPin()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/changePassword.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.checkRecoveryPin()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/changePasswordValidateCodeFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>Recover Password</description>
        <from-view-id>/changePassword.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.editPassword()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/changePasswordSuccess.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/afterLogin/productInfo.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{myProductManagedBean.checkProduct()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/editProductInfo.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{cartManagedBean.displayItem()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/client/bookTicket.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/editProductInfo.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{myProductManagedBean.editDetails()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/editProductInfoSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{myProductManagedBean.editDetails()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/editProductInfoFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/editProductInfoSuccess.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{myProductManagedBean.displayProduct()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/productInfo.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/findUser.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.displayDetails()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/afterLogin/userDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/addProduct.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{myProductManagedBean.addProduct()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/addProductSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{myProductManagedBean.addProduct()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/addProductFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/client/buyProduct.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{cartManagedBean.addItemtoCart()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/client/addItemSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{cartManagedBean.addItemtoCart()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/client/addItemFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/client/editCart.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{cartManagedBean.removeItem()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/client/deleteSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{cartManagedBean.removeItem()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/client/deleteFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/deleteProduct.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{myProductManagedBean.deleteProduct()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/deleteProductSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{myProductManagedBean.deleteProduct()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/deleteProductFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/client/checkYourCart.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{cartManagedBean.confirm()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/client/confirmPage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{cartManagedBean.confirm()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/client/confirmFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/createAdmin.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userManagedBean.addAdmin()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/createAdminSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{userManagedBean.addAdmin()}</from-action>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/createAdminFailure.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
